{
    "name": "gooddaykya/pdobase",
    "description": "PDOBase. Wrapper for PDO",
    "keywords": [
        "sql",
        "mysql",
        "pdo",
        "database wrapper"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "gooddaykya",
            "email": "makedonskii.net@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3",
        "ext-PDO": "*",
        "ext-pdo_mysql": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "suggest": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "gooddaykya\\components\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "gooddaykya\\tests\\": "tests/"
        }
    }
}
